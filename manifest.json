{
  "manifest_version": 3,
  "name": "Bookmark Manager (Staging)",
  "version": "1.1.0",
  "description": "Two-way bookmark staging with 24-hour expiration.",
  "permissions": ["bookmarks", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "manager.html"
}